{
  "name": "mqtthree",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@smui/button": "^7.0.0-beta.16",
    "@smui/card": "^7.0.0",
    "@smui/circular-progress": "^7.0.0-beta.16",
    "@smui/dialog": "^7.0.0-beta.16",
    "@smui/fab": "^7.0.0-beta.16",
    "@smui/textfield": "^7.0.0-beta.16",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.20.4",
    "@threlte/core": "latest",
    "@threlte/extras": "latest",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.5.7",
    "@types/paho-mqtt": "^1.0.7",
    "@types/three": "^0.152.1",
    "@types/uuid": "^9.0.7",
    "@vite-pwa/sveltekit": "^0.3.0",
    "smui-theme": "^7.0.0-beta.16",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "three": "^0.153.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "paho-mqtt": "^1.1.0",
    "uuid": "^9.0.1",
    "yaml": "^2.3.2"
  }
}
